{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "CS2 Enemy Intelligence",
    "version": "1.0.0",
    "author": "Gegenschlag Team"
  },
  "data": {
    "game_events": {
      "730": {
        "game_events": ["kill_feed", "match_info"],
        "match_events": ["match_start", "match_end"]
      }
    },
    "launch_events": [
        {
            "event": "GameLaunch",
            "event_data": {
                "game_ids": [730]
            },
            "start_minimized": false
        }
    ],
    "windows": {
        "background": {
            "file": "index.html",
            "transparent": true,
            "resizable": false,
            "show_in_taskbar": false,
            "click_through": true
        },
        "in_game": {
            "file": "index.html",
            "transparent": true,
            "resizable": false,
            "in_game_only": true,
            "focus_game_takeover": "Released"
        }
    },
    "permissions": ["GameInfo", "GameEvents", "Hotkeys", "Streaming"]
  }
}
